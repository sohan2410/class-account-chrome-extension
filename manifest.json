{
  "manifest_version": 2,
  "name": "Theme-Changer",
  "version": "2.0.1",
  "description": "This extension is basically dark theme",
  "author": "Sohan Bandary",
  "icons": {
    "518": "icon.png"
  },
  "browser_action": {
    "default_title": "Hello!"
  },
  "permissions": ["tabs", "storage", "notifications", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  }
}
